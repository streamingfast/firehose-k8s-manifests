{
  "apiVersion": "v1",
  "kind": "LimitRange",
  "metadata": {
    "name": "mem-limit-range",
    "namespace": "eth-mainnet"
  },
  "spec": {
    "limits": [
      {
        "default": {
          "cpu": "1",
          "memory": "512Mi"
        },
        "defaultRequest": {
          "cpu": "500m",
          "memory": "256Mi"
        },
        "type": "Container"
      }
    ]
  }
}
